{% import "_includes/forms" as forms %}

{{ forms.textField({
    first: true,
    label: "Varnish server IP",
    name: 'ip',
    value: settings.ip
}) }}

{{ forms.textField({
    label: "Varnish server PORT",
    name: 'port',
    value: settings.port
}) }}

{{ forms.textField({
    label: "Varnish server VERSION",
    name: 'version',
    value: settings.version
}) }}

{{ forms.textField({
    label: "Varnish server SECRET",
    name: 'secret',
    value: settings.secret
}) }}